   <children>
      <TextField fx:id="loginTextField" layoutX="219.0" layoutY="128.0" promptText="login" />
      <PasswordField fx:id="passwordTextField" layoutX="219.0" layoutY="172.0" promptText="password" />
      <Button fx:id="loginButton" layoutX="231.0" layoutY="211.0" mnemonicParsing="false" onAction="#onLoginClick" prefHeight="28.0" prefWidth="138.0" text="Login" />
      <Button fx:id="registrationButton" layoutX="232.0" layoutY="274.0" mnemonicParsing="false" onAction="#onRegistrationClick" prefHeight="28.0" prefWidth="138.0" text="Registrate" />
   </children>
</AnchorPane>
